{
  "scripts": {
    "develop:jekyll": "export JEKYLL_ENV=development && bundle exec jekyll serve --draft --livereload --incremental --watch",
    "develop:webpack": "export NODE_ENV=development && webpack --mode development --watch --progress --color",
    "build:prod": "export NODE_ENV=production && export JEKYLL_ENV=production && npm run build:webpack && npm run build:jekyll",
    "build:jekyll": "bundle exec jekyll build",
    "build:webpack": "webpack --progress --config webpack.config.js"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.12",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "stimulus": "^3.0.1",
    "tailwindcss": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "babel-loader": "^8.2.5",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
